{
    "name": "exdeliver/causeway",
    "description": "Content Management System for Laravel applications",
    "license": "MIT",
    "keywords": [
        "laravel",
        "php",
        "cms"
    ],
    "authors": [
        {
            "name": "Jason Hoendervanger",
            "email": "info@exdeliver.com"
        }
    ],
    "require": {
        "mollie/mollie-api-php": "2.0.*",
        "h4cc/wkhtmltopdf-amd64": "*",
        "h4cc/wkhtmltopdf-i386": "*",
        "akaunting/money": "^1.0",
        "laravelcollective/html": "^6.0",
        "mews/purifier": "~3.0",
        "rennokki/befriended": "^3.0",
        "laravel/framework": "^7.0",
        "spatie/laravel-permission": "^3.0",
        "vinkla/hashids": "^8.0",
        "beyondcode/laravel-websockets": "^1.0",
        "pusher/pusher-php-server": "^3.0",
        "yajra/laravel-datatables-oracle": "~9.0",
        "van-ons/laraberg": "^0.0.6@beta",
        "unisharp/laravel-filemanager": "~1.8",
        "astrotomic/laravel-translatable": "^11.8",
        "intervention/image": "^2.4",
        "owen-it/laravel-auditing": "^10.0",
        "barryvdh/laravel-snappy": "^0.4.3",
        "myparcelnl/sdk": "^4.0",
        "exdeliver/cart": "dev-master",
        "laravel/helpers": "^1.0",
        "laravel/ui": "^2.0"
    },
    "scripts": {
        "post-package-install": [
            "php artisan migrate",
            "php artisan vendor:publish --tag=public --force"
        ],
        "post-package-update": [
            "php artisan migrate",
            "php artisan vendor:publish --tag=public --force"
        ]
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "Exdeliver\\Causeway\\": "src/",
            "Exdeliver\\Causeway\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Exdeliver\\Causeway\\ServiceProviders\\CausewayServiceProvider"
            ]
        }
    }
}
